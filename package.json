{
  "name": "@11ty/eleventy",
  "version": "0.2.5",
  "description": "Transform a directory of templates into HTML.",
  "main": "src/Eleventy.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "eleventy": "./cmd.js"
  },
  "scripts": {
    "default": "cd playground && npx eleventy --input=. --output=_site",
    "test": "npx nyc ava",
    "watch:test": "npx ava --watch --verbose",
    "coverage":
      "npx nyc report --reporter=json-summary && cp coverage/coverage-summary.json docs-src/_data/coverage.json && npx eleventy --config=docs-src/.eleventy.docs.js",
    "precommit": "lint-staged",
    "prepush": "npx ava"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy.git"
  },
  "ava": {
    "files": ["test/*.js"],
    "source": ["**/.eleventyignore", "src/**/*.js"]
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "nyc": "^11.4.1",
    "prettier": "1.9.1"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "check-node-version": "^3.1.1",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "fs-extra": "^4.0.2",
    "glob-watcher": "^4.0.0",
    "globby": "^7.1.1",
    "gray-matter": "^3.1.1",
    "hamljs": "^0.6.2",
    "handlebars": "^4.0.11",
    "liquidjs": "^2.2.0",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isobject": "^3.0.2",
    "lodash.merge": "^4.6.0",
    "lodash.set": "^4.3.2",
    "luxon": "^0.2.12",
    "markdown-it": "^8.4.0",
    "minimist": "^1.2.0",
    "mustache": "^2.3.0",
    "normalize-path": "^2.1.1",
    "nunjucks": "^3.0.1",
    "parse-filepath": "^1.0.1",
    "pify": "^3.0.0",
    "pretty": "^2.0.0",
    "pug": "^2.0.0-rc.4",
    "slugify": "^1.2.7"
  }
}
