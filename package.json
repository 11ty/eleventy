{
  "name": "eleventy-cli",
  "version": "0.1.3",
  "description": "Transform a directory of templates into HTML.",
  "main": "src/Eleventy.js",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "eleventy": "./cmd.js"
  },
  "scripts": {
    "default": "node cmd.js",
    "watch": "node cmd.js --watch",
    "test": "ava",
    "watch:test": "ava --watch --verbose",
    "precommit": "lint-staged"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zachleat/eleventy.git"
  },
  "ava": {
    "files": ["test/*.js"],
    "source": ["**/.eleventyignore", "src/**/*.js"]
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "1.9.1"
  },
  "dependencies": {
    "ejs": "https://github.com/zachleat/ejs.git#v2.5.7-h1",
    "fs-extra": "^4.0.2",
    "glob-watcher": "^4.0.0",
    "globby": "^7.1.1",
    "gray-matter": "^3.1.1",
    "hamljs": "^0.6.2",
    "handlebars": "^4.0.11",
    "liquidjs": "^2.2.0",
    "lodash.chunk": "^4.2.0",
    "lodash.clone": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "lodash.set": "^4.3.2",
    "markdown-it": "^8.4.0",
    "minimist": "^1.2.0",
    "mustache": "^2.3.0",
    "normalize-path": "^2.1.1",
    "nunjucks": "^3.0.1",
    "parse-filepath": "^1.0.1",
    "pify": "^3.0.0",
    "pretty": "^2.0.0",
    "pug": "^2.0.0-rc.4",
    "slugify": "^1.2.7"
  }
}
