{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "jsxImportSource": "jsx-async-runtime",
    "module": "NodeNext",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [ "node_modules", "_site" ]
}
