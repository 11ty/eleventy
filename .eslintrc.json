{
	"env": {
			"es2021": true,
			"node": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"no-async-promise-executor": "warn",
		"no-prototype-builtins": "warn"
	}
}
